{
  "name": "resultwire",
  "version": "0.2.2",
  "description": "Rust-like Result types, easily serialized over the wire",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "release": "git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint": "eslint -c .eslintrc.cjs '**/*.ts'",
    "lint:fix": "eslint --fix -c .eslintrc.cjs '**/*.ts' --fix",
    "prepublishOnly": "npm run build",
    "build": "tsc"
  },
  "author": "nick-potts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nick-potts/ResultWire.git"
  },
  "bugs": {
    "url": "https://github.com/nick-potts/ResultWire/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "devDependencies": {
    "@js-lib/cli": "^2.3.4",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.1.0",
    "commitizen": "^4.2.4",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@commitlint/format": "^12.1.1",
    "@commitlint/prompt-cli": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "typescript": "^5.2.2",
    "coveralls": "^3.1.1",
    "expect.js": "^0.3.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
